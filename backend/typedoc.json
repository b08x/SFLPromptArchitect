{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts", "src/types.ts"],
  "out": "../../docs/backend",
  "plugin": ["typedoc-unhoax-theme"],
  "name": "Backend API",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "node_modules/**/*",
    "dist/**/*"
  ],
  "excludePrivate": true,
  "excludeExternals": true
}
